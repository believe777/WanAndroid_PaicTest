.mine_view {
  display: flex;
  flex-direction: column;
}

.mine_logo_view {
  width: 100%;
  height: 150px;
  background-color: #4c9e4c;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.header_icon {
  width: 50px;
  height: 50px;
  border-radius: 25px;
}

.header_login_text{
  color: #fff;
  font-size: 14px;
  padding: 10px;
}

.mine_item{
  display: flex;
  flex-direction: row;
  padding-left: 10px;
  padding-top: 15px;
  padding-bottom: 15px;
  padding-right: 10px;
  align-items: center;
}

.mine_item_icons{
  width: 35px;
  height: 35px;
  margin-right: 35px;
}

.mine_item_text{
  font-size: 14px;
  color: #666666;
}

.base_line{
  width: 100%;
  height: 1px;
  background-color: #cccccc;
}

page {
  background-color: #f8f8f8;
}
